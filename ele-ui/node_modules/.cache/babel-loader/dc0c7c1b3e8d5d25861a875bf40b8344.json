ize=\"pageSize\"\n                :total=\"total\"\n                @current-change=\"pageChange\"\n        >\n        </el-pagination>\n    </div>\n</template>\n\n<script>\n    export default {\n        methods: {\n            //分页方法,点击后触发事件\n            pageChange(currentPage) {\n                const _this = this\n                axios.get('http://localhost:8081/user/findAll/' + currentPage.toString() + \"/9\").then(function (resp) {\n                    _this.tableData = resp.data.records\n                    _this.pageSize = resp.data.size\n                    _this.total = resp.data.total\n                })\n            }\n        },\n\n        data() {\n            return {\n                pageSize: 0,\n                total: 0,\n                tableData: [{\n                    id: '888',\n                    name: '神圣领域',\n                    phone: '13888888888',\n                    email: 'percherIrex@gmail.com ',\n                    address: '上海市普陀区真北路',\n                    depart: '销售部',\n                    position: '部门主任',\n                    gender: '男',\n                    intime: '2018-03'\n                }]\n            }\n        },\n\n        created() {\n            const _this = this\n            _this.$forceUpdate()\n\n            axios.get(\"http://localhost:8081/user/findAll/1/9\").then(function (resp) {\n                _this.tableData = resp.data.records\n                _this.pageSize = resp.data.size\n                _this.total = resp.data.total\n            })\n        }\n    }\n</script>"],"sourceRoot":"src/views"}]}